{
    "version": 2,
    "builds": [
      {
        "src": "./src/api/contentData-api.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["./database/data.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./src/api/contentData-api.js"
      }
    ]
  }